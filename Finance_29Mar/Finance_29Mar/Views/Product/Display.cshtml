@model IEnumerable<Finance_29Mar.Models.Product>   
@{
    ViewData["Title"] = "Display";
}

@foreach (var item in Model)
{
    <p>Products Available are...</p>
    <body>
        <div>
            <img src=@item.Image height="500" width="800" alt=@item.ProductName />
            <p><b>@item.ProductName</b></p><br />
            <p>@item.Description</p><br />
            <h5>@item.ProductCost</h5>
            <p><strike class="text-danger">@item.StrikedCost </strike></p>
        </div>
    </body>
}
